<idea-plugin>
    <id>Tracker.TimeTracker</id>
    <name>MyTimeTracker</name>
    <vendor email="sayahov1998@gmail.com">MyCompany</vendor>
    <version>1.0</version>


    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <idea-version since-build="145.0"/>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>

    <project-components>
        <component>
            <implementation-class>TimeTrackerComponent</implementation-class>
        </component>
    </project-components>

    <application-components>
        <component>
            <implementation-class>TimeTrackerDefaultSettingsComponent</implementation-class>
        </component>
    </application-components>
</idea-plugin>